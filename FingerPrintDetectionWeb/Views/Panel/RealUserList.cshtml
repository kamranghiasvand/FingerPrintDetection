@model dynamic

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                لیست کاربران
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 validation-summary-errors hidden" style="color: red;" data-valmsg-summary="true">
                    </div>
                </div>
                <div class="row">
                    <table class="table table-striped table-bordered table-hover dataTable no-footer" width="100%"
                           data-url="@Url.Action("GetRealUsersListAsync", "Panel")" data-removeuser="@Url.Action("DeleteRealUser", "Panel")"
                           data-addfinger="@Url.Action("AddFinger", "Panel")"
                           data-edituser="@Url.Action("AddRealUser")"
                           id="UserdataTables"></table>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/Views/Panel/RealUserList.js"></script>
}
